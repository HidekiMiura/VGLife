<% @title = '登録情報編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
	<%= form_for @customer, url: :customer_account do |f| %>
	<%= render 'form', f: f %>
	<div class="buttons">
		<%= link_to 'キャンセル', :customer_account, :class => "cancel" %>&nbsp;&nbsp;
		<%= f.submit '更新' %>
	</div>
	<% end %>
</div>
