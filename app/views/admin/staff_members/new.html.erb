<% @title = '職員の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @staff_member ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= link_to 'キャンセル', :admin_staff_members, :class => "cancel" %>
      <%= f.submit '登録'%><br />
    </div>
  <% end %>
</div>