<% @title = '職員アカウントの編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for [ :admin, @staff_member ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= link_to 'キャンセル', :admin_staff_members ,:class =>"cancel" %>&nbsp;&nbsp;
      <%= f.submit '更新'%>
    </div>
  <% end %>
</div>