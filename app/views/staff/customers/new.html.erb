<% @title = '顧客の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @customer_form, as: 'form', url: :staff_customers do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= link_to 'キャンセル', :staff_customers %>
      <%= f.submit '登録'%><br />
    </div>
  <% end %>
</div>