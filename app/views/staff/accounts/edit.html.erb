<% @title = 'アカウント情報編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
	<%= form_for @staff_member, url: :staff_account do |f| %>
	<%= render 'form', f: f %>
	<div class="buttons">
		<%= link_to 'キャンセル', :staff_account, :class => "cancel" %>&nbsp;&nbsp;
		<%= f.submit '更新' %>
	</div>
	<% end %>
</div>
