<% @title = '野菜の編集' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @vegetable_form, as: 'form', url: [ :staff, @vegetable_form.vegetable ] do |f| %>
    <%= render 'form', f: f %>
	<div class="buttons">
		<%= link_to 'キャンセル', :staff_vegetables, :class => "cancel" %>&nbsp;&nbsp;
		<%= f.submit '変更' %>
	</div>
	<% end %>
</div>
